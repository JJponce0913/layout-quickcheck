
{
    "paths": {
        "bug-reports-directory": "./bug_reports",
        "tmp-files-directory": "./tmp_generated_files"
    },

    "variants": [
        {"type": "firefox",
         "target": true,
         "options": {"layout.css.grid-item-baxis-measurement.enabled": true}},
        {"type": "firefox",
         "options": {"layout.css.grid-item-baxis-measurement.enabled": false}}
    ],

    "style-weights": {
        "content-visibility": 0,
        "writing-mode": 0,
        "display": 100,

