{
    "style-weights": {

        "display": 100,

        "display:inline": 0,
        "display:table": 0,
        "display:grid": 100,

        "grid-template-areas": 10, // Keyword = None
        "grid-template-columns": 10, // Keyword = None
        "grid-template-rows": 10, // Keyword = None

        "column-gap": 10, // Length, Percentage, Keyword = Normal
        "row-gap": 10, // Length, Percentage, Keyword = Normal

        "grid-column-start": 10, // Keyword = Auto
        "grid-column-end": 10, // Keyword = Auto
        "grid-row-start": 10, // Keyword = Auto
        "grid-row-end": 10, // Keyword = Auto

        "grid-auto-flow": 10, // Keyword = [row, column]
        "grid-auto-row": 10, // Keyword, Length, Percentage, Flex
        "grid-auto-col": 10, // Keyword, Length, Percentage, Flex
    }
}